{
  "name": "ngx-recursive-dropdown",
  "version": "1.0.3",
  "description": "A dropdown list using ng-template and ng-container in a recursive manner.",
  "main": "bundles/ngx-recursive-dropdown.umd.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ngx-recursive-dropdown.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngx-recursive-dropdown.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "author": "Nishant Hooda <hoodanishant1995@gmail.com>",
  "license": "MIT",
  "readme": "Re",
  "repository": "github:nishant95/ngx-recursive-dropdown",
  "keywords": [
    "dropdown",
    "ng-template",
    "recursive",
    "treeview",
    "angular"
  ],
  "peerDependencies": {
    "@angular/compiler": "^4.0.3",
    "@angular/core": "^4.1.0",
    "@angular/common": "^4.1.0",
    "@angular/forms": "^4.1.0",
    "@angular/platform-browser": "^4.1.0",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.9"
  }
}
