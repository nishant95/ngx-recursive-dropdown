{
  "name": "ngx-recursive-dropdown",
  "version": "1.0.8",
  "description": "A dropdown list using ng-template and ng-container in a recursive manner.",
  "main": "ngx-recursive-dropdown.umd.js",
  "jsnext:main": "ngx-recursive-dropdown.esm.js",
  "module": "ngx-recursive-dropdown.esm.js",
  "types": "ngx-recursive-dropdown.d.ts",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ngx-recursive-dropdown.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngx-recursive-dropdown.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "author": "Nishant Hooda <hoodanishant1995@gmail.com>",
  "license": "MIT",
  "repository": "github:nishant95/ngx-recursive-dropdown",
  "keywords": [
    "dropdown",
    "ng-template",
    "recursive",
    "treeview",
    "angular"
  ],
  "homepage": "https://github.com/nishant95/ngx-recursive-dropdown#readme",
  "dependencies": {
    "@angular/compiler": "^4.0.3",
    "@angular/core": "^4.1.0",
    "@angular/common": "^4.1.0",
    "@angular/forms": "^4.1.0",
    "@angular/platform-browser": "^4.1.0",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.9"
  },
  "devDependencies": {
    "@angular/compiler": "^4.4.7",
    "@angular/compiler-cli": "^4.4.7",
    "@types/node": "^7.0.14",
    "del": "^2.2.2",
    "node-sass": "^4.5.3",
    "rollup": "^0.58.2",
    "rollup-plugin-angular": "^0.4.4",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "typescript": "^2.8.3",
    "uglify-js": "^3.3.25"
  }
}
